module compare (
    input z,
    input v,
    input n,
    input alufn[6],
    output cmp
) {
    mux_4 compare_mux
    always {
        cmp = 0
        
        compare_mux.s0 = alufn[1]
        compare_mux.s1 = alufn[2]
        compare_mux.in[0] = 0 
        compare_mux.in[1] = z
        compare_mux.in[2] = n ^ v
        compare_mux.in[3] = (n ^ v) | z
        
        cmp = compare_mux.out
    }
}